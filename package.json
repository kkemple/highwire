{
  "name": "@kkemple/highwire",
  "version": "1.0.2",
  "description": "A high level HTTP client",
  "main": "dist/index.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint .",
    "test": "node_modules/.bin/tape -r babel-register test/*.js | faucet",
    "compile": "node_modules/.bin/rimraf dist && node_modules/.bin/babel lib --out-dir dist",
    "dev": "node_modules/.bin/rimraf dist && node_modules/.bin/babel lib --out-dir dist -w",
    "release": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkemple/highwire.git"
  },
  "keywords": [
    "http",
    "bluebird",
    "promises",
    "requests",
    "network",
    "api",
    "client",
    "request",
    "superagent",
    "highwire",
    "network",
    "requests"
  ],
  "author": "kurtiskemple@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkemple/highwire/issues"
  },
  "homepage": "https://github.com/kkemple/highwire#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "faucet": "0.0.1",
    "nock": "^5.2.1",
    "rimraf": "^2.5.0",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "backoff": "^2.4.1",
    "bluebird": "^3.1.1",
    "lodash.assign": "^4.0.0",
    "superagent": "^1.6.1",
    "superagent-retry": "^0.5.1"
  }
}
